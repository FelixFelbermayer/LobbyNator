val.addEventListener('click', function(){\n            toggleClass(val, 'active')\n        })\n    })\n\n    search.addEventListener('keyup', function(){\n        filterInputs(search.value)\n    })\n})"},"lineCount":null}},"error":null,"hash":"38c97a7e30ba2bf0dcb681b51ef3d072","cacheData":{"env":{}}}